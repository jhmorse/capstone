@Article{Westall2023,
  author    = {Westall, John and Cummings, Amy},
  journal   = {Education Policy Innovation Collaborative},
  title     = {The Effects of Early Literacy Policies on Student Achievement.},
  year      = {2023},
  comment   = {A focus on early literacy programs and their impact on student literacy by the end of third grade. Outcomes are focused on standardized test scores, both high-stakes and low-stakes testing. High-stakes testing outcomes may impact student plaement, teacher evaluations, or school ratings/funding. There is an implication that high-stakes outcomes may be the result of "teaching to the test." Low-stakes testing, on the other hand, have no direct consequences and are used pirmarily for internal monitoring of progress. The authors conclude early literacy programs can significantly impact high-stakes testing but have little impact on low-stakes testing.},
  publisher = {ERIC},
  url       = {https://eric.ed.gov/?id=ED672706},
}

