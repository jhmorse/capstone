@Article{Long2012,
  author    = {Long, Mark C. and Conger, Dylan and Iatarola, Patrice},
  journal   = {American Educational Research Journal},
  title     = {Effects of High School Course-Taking on Secondary and Postsecondary Success},
  year      = {2012},
  issn      = {0002-8312},
  number    = {2},
  pages     = {285-322},
  volume    = {49},
  comment   = {An assessment of student test scores based on participation in advanced curriculum including Honors, Advanced Placement (AP), and other rigorous courses. The study focuses on Florida students graduating from high school in 2003. The study includes an examination of enrollment in 2-year or 4-year colleges after graduation and, as such, represents a similar analysis as proposed by my hypothesis. However, this study examines tests related to advanced and dual-credit courses such as Advanced Placement (AP) and Honors. Enrollment in ELA based advanced courses in high school demonstrated significant increases in college credits earned, grade point average, and the rate at which students earned a bachelor's degree.},
  doi       = {10.3102/0002831211431952},
  file      = {:https\://www.semanticscholar.org/paper/8556cfb76aca7958092a397f2005d9cfdeaf1e50:},
  publisher = {American Educational Research Association (AERA)},
  url       = {https://www.semanticscholar.org/paper/8556cfb76aca7958092a397f2005d9cfdeaf1e50},
}

@Article{Ozek2025,
  author    = {{Özek, Umut}},
  journal   = {EdWorkingPaper},
  title     = {The Effects of High School Remediation on Long-Run Educational Attainment},
  year      = {2025},
  number    = {1204},
  volume    = {25},
  comment   = {A study of the effects of placing students in remedial reading programs in school. Such programs can be beneficial to students to bring them up to reading standards as long as the student is not "tracked" into a remedial program, i.e. restricted to remedial classes across other subjects and as they advance in school. The article is included as a counterpoint to students that demonstrate higher literary achievement. Additionally, enrollment in remedial courses can indicate lowered literacy expectations for college admissions and could alter the influence of literacy achievement in secondary schools.},
  doi       = {10.26300/5A3Y-AQ41},
  publisher = {Retrieved frfom Annenberg Institute at Brown University},
  url       = {https://doi.org/10.26300/5a3y-aq41},
}

@Article{Savolainen2008,
  author    = {Hannu Savolainen and Timo Ahonen and Mikko Aro and Asko Tolvanen and Leena Holopainen},
  journal   = {Learning and Instruction},
  title     = {Reading comprehension, word reading and spelling as predictors of school achievement and choice of secondary education},
  year      = {2008},
  issn      = {0959-4752},
  month     = apr,
  number    = {2},
  pages     = {201-210},
  volume    = {18},
  comment   = {An analysis of students in Finland and the role of reading and spelling skills as those students pursued upper secondary education. Students, upon entering the upper secondary level of education, similar to high school in the US, have the choice of pursuing general education or vocational education. This article analyzes what role, if any, reading and spelling have on that choice and subsequent career choices. Literacy in lower grade levels is demonstrated to affect school choices in upper grade levels which, ultimately, affect post-secondary pursuits (college vs. vocational career). The relationship directly impacts college enrollment through a mid-level choice when entering the equivalent of high school.},
  doi       = {10.1016/j.learninstruc.2007.09.017},
  publisher = {Elsevier BV},
}

@Report{Hein2013,
  author      = {Hein, Vanessa and Smerdon, Becky and Sambolt, Megan},
  comment     = {A literature review highlighting indicators and predictors of student success, including success in reading and writing skills. Tracks primary predictors of success at multiple levels: elementary, middle school, high school, and post-graduation. Test scores, attendance, and GPA are included as primary indicators of success, while enrollment in advanced courses and social skills also contribute to success. Intended to support the design and implementation of programs by school policymakers toward improving student performance and outcomes. The authors point out the difficulty in relating early achievement in school to post-secondary success. However, this research does note a relationship between early literacy and stadardized test scores, more specifically scores on college entrance testing (ACT and SAT). Additionally, higher scores on AP and IB exams are correlated with higher rates of college enrollment.},
  institution = {College and Career Readiness and Success Center, American Institutes for Research},
  month       = nov,
  number      = {ED555671},
  pages       = {16},
  title       = {Predictors of Postsecondary Success},
  url         = {https://files.eric.ed.gov/fulltext/ED555671.pdf},
  urldate     = {2026-01-23},
  year        = {2013},
}

@Article{Fidalgo2014,
  author    = {Fidalgo, Raquel and Torrance, Mark and Arias-Gund{\'i}n, Olga and Mart{\'i}nez-Coc{\'o}, Bego{\~n}a},
  journal   = {Psicothema},
  title     = {Comparison of reading--writing patterns and performance of students with and without reading difficulties},
  year      = {2014},
  issn      = {0214-9915},
  number    = {4},
  pages     = {442--448},
  volume    = {26},
  comment   = {The authors analyze reading comprehension as a hybrid task that involves multiple activities including reading, comprehending, and writing. A common example is analyzing and summarizing an article after reading it. In the analysis, the authors compare students with and without reading difficulties to understand the comprehension process and the iterative cycles used to understand and write about a topic. Of most relevance in this study is the conclusion that reading comprehension strategies do not vary as children age. Students were studied at the primary and secondary level in Spain, where the primary level includes 5th and sixth grades, while the secondary level equates to US high school. This demonstrates a consistency in literacy activities across grades and may indicate achievement scores will stay consistent as a child ages.},
  publisher = {Universidad de Oviedo},
  url       = {https://www.redalyc.org/pdf/727/72732458002.pdf},
  urldate   = {2026-01-23},
}

@InProceedings{Graham2010,
  author    = {Graham, Susan and Carroll, Wendy R.},
  booktitle = {Proceedings of the 40th Atlantic Schools of Business Conference},
  title     = {Exploring the Impact of Literacy on Business Education and Job-Readiness: An Examination of the Reading Comprehension Skills of First Year University Students},
  year      = {2010},
  address   = {Halifax, Nova Scotia},
  pages     = {487--498},
  publisher = {Atlantic Schools of Business},
  comment   = {A secondary analysis of survey results collected as part of a doctoral dissertation and subsequently delivered as part of a 2010 conference. The authors review and analyze the reading comprehension of first-year university students and compare against the general population in Canada as well as comparing within group by gender and by area of major study (arts, science, and business administration). Crucially, the authors determined no difference in reading comprehension from the Canadian population as a whole, indicating that literacy skills were not a factor in university enrollment. Applied broadly, universities looking to attract students may disregard literacy skills, potentially impacting student readiness and success in college.},
  url       = {https://www.academia.edu/129669350/Battle_of_narratives_in_a_changing_FMCG_distribution_channel},
  urldate   = {2026-01-25},
}

@Article{Cox2003,
  author    = {Steven R. Cox and Daniel L. Friesner and Mohammed Khayum},
  journal   = {Journal of College Reading and Learning},
  title     = {Do Reading Skills Courses Help Underprepared Readers Achieve Academic Success in College?},
  year      = {2003},
  issn      = {2332-7413},
  month     = mar,
  number    = {2},
  pages     = {170-196},
  volume    = {33},
  comment   = {An examination of university students entering college without the requisite literacy (reading comprehension and writing) skills to succeed. Students are identified based on test scores and grouped based on students who do not take a reading skills course, those who take a course but do not pass, and those that take a course and pass and/or get an exceptional grade (A). The authors conclude that under-prepared students are more likely to succeed in college if they have taken and passed a reading skills course. This research demonstrates the importance of literacy skills for first-year students in college and leverages data from a reading placement test to analyze student readiness.},
  doi       = {10.1080/10790195.2003.10850147},
  publisher = {Informa UK Limited},
  url       = {https://doi.org/10.1080/10790195.2003.10850147},
}

@Article{James2024,
  author    = {James, Emma and Thompson, Paul A. and Bowes, Lucy and Nation, Kate},
  journal   = {Journal of Educational Psychology},
  title     = {What are the long-term prospects for children with comprehension weaknesses? A registered report investigating education and employment outcomes.},
  year      = {2024},
  issn      = {0022-0663},
  month     = aug,
  number    = {6},
  pages     = {1019--1033},
  volume    = {116},
  comment   = {In this analysis, the authors analyze the educational and employment outcomes of children identified with poor reading comprehension while in elementary school. This article recognizes the impact reading comprehension issues can have on long term outcomes, even when they are identified early in the child's education (e.g. 3rd or 4th grade). The analysis is a comparison of students identified as having low reading comprehension ability with students at or above expectations. Secondary school capability is linked to post-secondary success in both education and employment. The authors also look at assessment scores (SATs) as an outcome variable.},
  doi       = {10.1037/edu0000898},
  publisher = {American Psychological Association (APA)},
  url       = {https://psycnet.apa.org/fulltext/2025-05519-001.html},
}

@Article{Talwar2022,
  author    = {Talwar, Amani and Magliano, Joseph P. and Higgs, Karyn and Santuzzi, Alecia and Tonks, Stephen and O’Reilly, Tenaha and Sabatini, John},
  journal   = {Journal of College Reading and Learning},
  title     = {Early Academic Success in College: Examining the Contributions of Reading Literacy Skills, Metacognitive Reading Strategies, and Reading Motivation},
  year      = {2022},
  issn      = {2332-7413},
  month     = oct,
  number    = {1},
  pages     = {58--87},
  volume    = {53},
  comment   = {Examines mulitple facets of literacy: literacy skills (including vocabulary and reading comprehension), reading strategies, and reading motivation. The research analyzes how these multiple aspects of literacy affect academic reading and student success in college. The authors introduce a new framework for assessing literacy skills. Individual students were asked to complete a custom assessment for the purposes of the research. Outcomes demonstrated the direct impact of literacy skills on college success, indicating readiness obtained prior to college directly influences that success.},
  doi       = {10.1080/10790195.2022.2137069},
  publisher = {Informa UK Limited},
}

@Article{Hernandez2011,
  author    = {Hernandez, Donald J},
  journal   = {Annie E. Casey Foundation},
  title     = {Double jeopardy: How Third-Grade Reading Skills and Poverty Influence High School Graduation.},
  year      = {2011},
  comment   = {An overview of high school drop out rates linked to both literacy in third grade and poverty rates. As a link between early literacy skills and later secondary success, this article demonstrates the continuation of impact across grades as children get older. Third-grade reading assessments for the students used in this analysis are based on national scores collected through standardized testing including precursor testing to standard implemented through No Child Left Behind like SBAC.},
  publisher = {ERIC},
  url       = {https://www.aecf.org/resources/double-jeopardy},
}

